        if (this.isLoading) {\n            this.list.push({urls: urls, loadComplete: loadComplete});\n            return;\n        }\n        this.isLoading = true;\n        this.loadComplete = loadComplete;\n        if (urls.length == 0) {\n            this.resLoadComplete();\n        } else {\n            this.resLoadProgress(0);\n            Laya.loader.on(Event.ERROR, this, this.resLoadError);\n            Laya.loader.load(urls, Handler.create(this, this.resLoadComplete), Handler.create(this, this.resLoadProgress, null, false));\n        }\n    }\n\n    private loadNext(): void {\n        if (this.list.length > 0) {\n            var queueInfo: object = this.list.shift();\n            var urls: Array<any> = queueInfo[\"urls\"];\n            var loadComplete: any = queueInfo[\"loadComplete\"];\n            this.load(urls, loadComplete);\n        }\n    }\n\n    private resLoadError(): void {\n        Laya.loader.off(Event.ERROR, this, this.resLoadError);\n    }\n\n    private resLoadComplete(): void {\n        Laya.loader.off(Event.ERROR, this, this.resLoadError);\n        if (this.loadComplete) {\n            GameUtils.runCallBack(this.loadComplete);\n            this.loadComplete = null;\n        }\n        this.isLoading = false;\n        this.loadNext();\n    }\n\n    protected resLoadProgress(progress: number): void {\n        this.view.progressBar.value = progress;\n        this.view.labProgress.text = GameUtils.formatPercent(progress);\n    }\n\n    protected onStageResize(): void {\n        this.size(Laya.stage.width, Laya.stage.height);\n        this.graphics.clear();\n        this.graphics.drawRect(0, 0, this.width, this.height, \"#000000\");\n        this.pos(0, 0);\n        this.view.size(this.width, this.height);\n        this.view.background.width = this.width\n        this.view.background.height = this.height\n        // if (ModelUser.ins.loadingBg == null || ModelUser.ins.loadingBg.length == 0) {\n        //     ModelUser.ins.loadingBg = \"https://