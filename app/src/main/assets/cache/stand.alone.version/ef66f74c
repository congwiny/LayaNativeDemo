I.width = PlayerMessage.messageUI.imgBg.width = PlayerMessage.messageUI.labMsg.textField.textWidth + 62;\n\t\tPlayerMessage.messageUI.x = Laya.stage.width / 2;\n\t\tPlayerMessage.messageUI.y = 100;\n\t\tPlayerMessage.messageUI.alpha = 0;\n\n\t\tvar showComplete: Function = function (): void {\n\t\t\tTween.to(PlayerMessage.messageUI, { alpha: 0, y: PlayerMessage.messageUI.y - 50 }, 200, Ease.sineOut, Handler.create(PlayerMessage, hideComplete), playerMsgVo.liveTime);\n\t\t}\n\t\tvar hideComplete: Function = function (): void {\n\t\t\tPlayerMessage.floatNext();\n\t\t}\n\t\tTween.to(PlayerMessage.messageUI, { alpha: 1, y: PlayerMessage.messageUI.y - 50 }, 200, Ease.sineOut, Handler.create(PlayerMessage, showComplete));\n\t}\n\t//展示滚动消息\n\tpublic static showMessage(playerVo:PlayerVo,msg: string, liveTime: number = 1500): void {\n\t\tif (GameUtils.stringIsEmpty(msg)) {\n\t\t\treturn;\n\t\t}\n\t\tvar playerMsgVo: PlayerMsgVo = new PlayerMsgVo();\n\t\tplayerMsgVo.playerVo=playerVo;\n\t\tplayerMsgVo.msg = msg;\n\t\tplayerMsgVo.liveTime = liveTime;\n\t\tplayerMsgVo.time = Laya.Browser.now();\n\t\tPlayerMessage.list.push(playerMsgVo);\n\t\tif (!PlayerMessage.playing) {\n\t\t\tPlayerMessage.floatNext();\n\t\t}\n\t}\n}\n","import MapUnit from \"./MapUnit\";\nimport MapScene from \"./MapScene\";\nimport AniEvent from \"../animation/AniEvent\";\nimport GameAni from \"../animation/GameAni\";\nimport { AniAction } from \"../animation/AniAction\";\nimport AnimationKey from \"../constant/AnimationKey\";\n\nexport default class EffectUnit extends MapUnit{\n    public static curuid:number = 0;\n    private avatar:GameAni;\n    public uid:number;\n\n    constructor(mapScene:MapScene)\n    {\n        super(mapScene);\n        EffectUnit.curuid ++;\n        this.uid = EffectUnit.curuid;\n        this.avatar=new GameAni();\n        this.avatar.on(AniEvent.PLAY_END,this,this.onAniPlayEnd);\n        this.addChild(this.avatar);\n    }\n\n    public init(resId:string):void\n    {\n        this