BackRate(value: number) {\n\t\tthis._playBackRate = value;\n\t\tif (this._skeleton) {\n\t\t\tthis._skeleton.playbackRate(this._playBackRate);\n\t\t}\n\t}\n\tpublic loadResComplete(resInfo: SpineResVo): void {\n\t\t//已经销毁掉了 就不用再处理动画相关逻辑了\n\t\tif (this.destroyed) return;\n\t\t//_resID已经改变了就不要执行了\n\t\tif (resInfo.path.indexOf(this._resID) == -1) {\n\t\t\treturn;\n\t\t}\n\t\tthis._templet = resInfo.templet;\n\t\tthis.initSkeleton();\n\t\tif (this._skeleton != null) {\n\t\t\tthis._skeleton.playbackRate(this._playBackRate);\n\t\t\tthis._skeleton.scale(this._extraScale, this._extraScale);\n\t\t\tif (this._playing) {\n\t\t\t\tthis._skeleton.play(this._action, this._loop, this._force, this._start, this._end, this._freshSkin);\n\t\t\t}\n\t\t\tthis.updatePauseStatus();\n\t\t}\n\t\tthis.event(SpineEvent.LOAD_COMPLETE);\n\n\t}\n\tpublic loadResError(resInfo: SpineResVo): void {\n\t\tLogger.log(\"动画加载失败,url=\" + resInfo.path);\n\t}\n\tprotected initSkeleton(): void {\n\t\tthis.disposeSkeleton();\n\t\tthis._skeleton = SpineControl.popSkeleton(this._templet);\n\t\tthis._skeleton.on(Event.STOPPED, this, this.onEndEvent);\n\t\tthis._skeleton.on(Event.LABEL, this, this.onLabelEvent);\n\t\tthis.addChildAt(this._skeleton, 0);\n\t}\n\tprotected onEndEvent(): void {\n\t\tif (this._autoDestroy) {\n\t\t\tthis.destroy();\n\t\t}\n\t\tthis.event(SpineEvent.PLAY_END);\n\t}\n\tprotected onLabelEvent(eventData: EventData): void {\n\t\tif (eventData.name.indexOf(\"play_\") >= 0) {\n\t\t\tvar info: Array<any> = eventData.name.split(\"_\");\n\t\t\tvar action: string = info[1];\n\t\t\tvar loop: number = 1;\n\t\t\tif (info[2] != null) {\n\t\t\t\tloop = <number>info[2];\n\t\t\t}\n\t\t\tif (action) {\n\t\t\t\tthis.play(action, loop == 1);\n\t\t\t}\n\t\t}\n\t\tthis.event(SpineEvent.LABEL,eventData);\n\t}\n\tpublic set paused(value: boolean) {\n\t\tthis._paused = value;\n\t\tthis.updatePauseStatus();\n\t}\n\tprivate updatePauseStatus(): void {\n\t\tif (this._skeleton != null && this._playing) {\n\t\t\tvar flag: boolean = this._paused || !this.displayedInStage;\n\t\t\tif (flag) {\n\t\t\t\tthis._skeleton.paused();\n\t\t\t}\n\t\t\telse {\n\t\t\t\tthis._skeleton.resume();\n\t\t\t}\n\t\t}\n\t}\n\tpublic get puased(): boolean {\n\t\treturn this._paused;\n\t}\n\tprotected disposeSkeleton(): void {\n\t\tif (this._skeleton) {\n\t\t\tthis._skeleton.removeSelf();\n\t\t\tthis._skeleton.off(Event.STOPPED, this, this.onEndEvent);\n\t\t\tthis._skeleton.off(Event.LABEL, this, this.onLabelEvent);\n\t\t\tthis._skeleton.stop();\n\t\t\tSpineControl.recoverSkeleton(this._skeleton);\n\t\t\tthis._skeleton = null;\n\t\t}\n\t}\n}\n","import LangUtils from '../../utils/LangUtils';\nimport PathManager from '../../managers/PathManager';\nimport ConfigManager from '../../managers/ConfigManager';\nimport ConfigKey from '../../constant/ConfigKey';\nimport SheetConfig from '../core/SheetConfig';\nimport StructConfig from '../core/StructConfig';\nimport ColorPredef from '../../constant/ColorPredef';\nexport class ItemConfig extends StructConfig {\n\tpublic id: number;\n\t//名字\n\tprivate _name: string;\n\t//信息\n\tprivate _info: string;\n\t//类型 ItemType \n\tpublic type: number;\n\t//图标ID\n\tprivate _icon: string;\n\t//品质\n\tpublic quality: number;\n\t//功能\n\tpublic func: number;\n\t//参数\n\tpublic parameter: string;\n\tpublic get name(): string {\n\t\treturn LangUtils.getLang(this._name);\n\t}\n\tpublic set name(value: string) {\n\t\tthis._name = value;\n\t}\n\tpublic get info(): string {\n\t\treturn LangUtils.getLang(this._info);\n\t}\n\tpublic set info(value: string) {\n\t\tthis._info = value;\n\t}\n\tpublic get icon(): string {\n\t\treturn PathManager.getIconsPath(\"item001\");\n\t}\n\tpublic set icon(value: string) {\n\t\tthis._icon = value;\n\t}\n}\nexport default class ItemTable extends SheetConfig {\n\tprivate static _ins: ItemTable;\n\tpublic static get ins(): ItemTable {\n\t\treturn ItemTable._ins = ItemTable._ins || new ItemTable();\n\t}\n\tconstructor(){\n        super();\n        ConfigMan