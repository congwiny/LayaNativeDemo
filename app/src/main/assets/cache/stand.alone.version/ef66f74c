nager{\n    public status:EntityBuildStatus;\n    private proxy:GameAni;\n    private config:EntityConfig;\n    private layer:Sprite;\n    public cacheEntityUnit:EntityUnit;\n    public buildAreaW:number;\n    constructor(){\n        this.status=EntityBuildStatus.None;\n        this.buildAreaW = 0;\n        this.layer=new Sprite();\n        this.layer.size(Laya.stage.width,Laya.stage.height);\n        this.layer.mouseEnabled=true;\n        this.layer.zOrder=-1;\n        this.layer.visible=false;\n        this.layer.on(Event.CLICK,this,this.layerClick);\n        LayerManager.ins.mainUILayer.addChild(this.layer);\n        this.cacheEntityUnit=null;\n    }\n    private layerClick():void{\n        var mapScene:MapScene=<MapScene>SceneManager.ins.currentScene;\n        if(this.status==EntityBuildStatus.Build){\n            var mapPos:Point=this.getMouseMapPos();\n            if(mapScene.checkCanPutEntity(mapPos,this.config) == false){\n                FloatMessage.showMessage(\"此处无法摆放\");\n            }else{\n                GameHandler.ins.send_buildEntity(this.config.id,Math.round(mapPos.x),Math.round(mapPos.y));\n            }\n        }\n    }\n\n    public setBuildAreaW(w:number):void\n    {\n        this.buildAreaW = w;\n        this.layer.size(Laya.stage.width-this.buildAreaW,Laya.stage.height);\n    }\n\n    public onStageResize():void{\n        this.layer.size(Laya.stage.width-this.buildAreaW,Laya.stage.height);\n    }\n    public onMouseMove():void{\n        var mapScene:MapScene=<MapScene>SceneManager.ins.currentScene;\n        if(mapScene == null)\n        {\n            return;\n        }\n\n        if(this.status==EntityBuildStatus.Build){\n            if (this.checkMouseInBuildArea())\n            {\n                this.proxy.visible = false;\n                StageManager.restoreCursor();\n            }\n            else\n            {\n                StageManager.changeCursor(\"\");\n                this.proxy.visible = true;\n                this.proxy.pos(Laya.stage.mouseX,Laya.stage.mouseY);\n                var mapPos:Point=this.getMouseMapPos();\n                if(mapScene.checkCanPutEntity(mapPos,this.config) == false){\n                    FilterUtils.setColorFilter(this.proxy,2,1,1);\n                }else{\n                    FilterUtils.setColorFilter(this.proxy,1,1,1);\n                }\n            }\n        }\n        else if (this.status==EntityBuildStatus.Clear)\n        {\n            if (this.checkMouseInBuildArea())\n            {\n                StageManager.restoreCursor();\n            }\n            else\n            {\n                StageManager.changeCursor(PathManager.getCommonPath(\"eraser\"));\n            }\n        }\n    }\n\n    private checkMouseInBuildArea():boolean\n    {\n        return Laya.stage.mouseX > Laya.stage.width - this.buildAreaW;\n    }\n\n    private getMouseTile():Point{\n        var mapScene:MapScene=<MapScene>SceneManager.ins.currentScene;\n        var mapCoord:Point=mapScene.screenToMapCoord(Laya.stage.mouseX,Laya.stage.mouseY);\n        return mapScene.getPosTile(mapCoord.x,mapCoord.y);\n    }\n\n    private getMouseMapPos():Point\n    {\n        var mapScene:MapScene=<MapScene>SceneManager.ins.currentScene;\n        var mapCoord:Point=mapScene.screenToMapCoord(Laya.stage.mouseX,Laya.stage.mouseY);\n        return mapCoord;\n    }\n\n    public toBuild(config:EntityConfig):void{\n        this.cancelBuild();\n        this.cancelClear();\n        this.config=config;\n        this.lockScreen();\n        if(!this.proxy){\n            this.proxy=new GameAni();\n            this.proxy.once(AniEvent.LOAD_COMPLETE,this,this.onLoadComplete)\n            this.proxy.zOrder=GameZOrder.MouseCursor;\n            this.proxy.init(config.res);\n            this.proxy.pos(Laya.stage.mouseX,Laya.stage.mouseY);\n            this.proxy.scale(0.75,0.75);\n            Laya.stage.addChild(this.proxy);\n        }\n        StageManager.changeCursor(\"\");\n        this.status=EntityBuildStatus.Build;\n    }\n\n    public onLoadComplete()\n    {\n        if (this.config[\"rect\"] == null)\n        {\n            var rect:Laya.Rectangle = this.proxy.getSelfBounds().clone();\n            this.config[\"rect\"] = rect;\n        }\n    }\n\n    public toClear():void{\n        this.cancelBuild();\n        this.cancelClear();\n        //this.lockScreen();\n        StageManager.changeCursor(PathManager.getCommonPath(\"eraser\"));\n        this.status=EntityBuildStatus.Clear;\n        var mapScene:MapScene=<MapScene>SceneManager.ins.currentScene;\n        mapScene.setAllEntitysHitArea(false);\n        mapScene.setAllPlayersHitArea(true);\n    }\n    private lockScreen():void{\n        this.layer.visible=true;\n    }\n    private unlockScreen():void{\n        this.layer.visible=false;\n    }\n    public cancelBuild():void{\n        if(this.status!=EntityBuildStatus.Build){\n            return;\n        }\n        if(this.proxy){\n            this.proxy.destroy();\n            this.proxy=null;\n        }\n        this.cacheEntityUnit=null;\n        this.config=null;\n        StageManager.restoreCursor();\n        this.unlockScreen();\n        this.status=EntityBuildStatus.None;\n    }\n    public cancelClear():void{\n        if(this.status!=EntityBuildStatus.Clear){\n            return;\n        }\n        this.cacheEntityUnit=null;\n        this.config=null;\n        StageManager.restoreCursor();\n        this.unlockScreen();\n        var mapScene:MapScene=<MapScene>SceneManager.ins.currentScene;\n        mapScene.setAllEntitysHitArea(true);\n        mapScene.setAllPlayersHitArea(false);\n        this.status=EntityBuildStatus.None;\n    }\n    private static _ins: EntityManager = null;\n    public static get ins(): EntityManager {\n        return EntityManager._ins = EntityManager._ins || new EntityManager();\n    }\n}","import MapUnit from \"./MapUnit\";\nimport PlayerVo from \"../model/map/PlayerVo\";\nimport GameAni from \"../animation/GameAni\";\nimport {editor} from \"../editor/layaMaxUI\";\nimport MapScene from \"./MapScene\";\nimport RandomUtils from \"../utils/RandomUtils\";\nimport {RoleDir, BehaviorType, InteractType} from '../constant/GameEnum';\nimport Point = Laya.Point;\nimport {AniAction} from \"../animation/AniAction\";\nimport FaceTable, {FaceConfig} from \"../configs/face/FaceTable\";\nimport TimeUtils from '../utils/TimeUtils';\nimport Rectangle = Laya.Rectangle;\nimport Event = Laya.Event;\nimport ModelUser from '../model/user/ModelUser';\nimport TipUIManager from \"../managers/TipUIManager\";\nimport TipUIKey from \"../constant/TipUIKey\";\nimport {LocationVo} from \"../views/core/TipUIBase\";\nimport PerformTable, {PerformConfig} from \"../configs/perform/PerformTable\";\nimport InteractTable, {InteractConfig} from \"../configs/interact/InteractTable\";\nimport AniEvent from '../animation/AniEvent';\nimport MathUtils from '../utils/MathUtils';\nimport FloatMessage from '../utils/FloatMessage';\nimport GameHandler from '../protocol/handler/GameHandler';\nimport Logger from '../utils/Logger';\nimport EntityManager, {EntityBuildStatus} from \"../managers/EntityManager\";\nimport AnimationKey from \"../constant/AnimationKey\";\nimport PlayerEvent from \"../events/PlayerEvent\";\nimport GameManager from \"../managers/GameManager\";\nimport FilterUtils from \"../utils/FilterUtils\";\nimport ModelMap from \"../model/map/ModelMap\";\nimport GlobalVars from \"../GlobalVars\";\n\nexport enum PlayerStatus {\n    Idle,\n    UpdateLocation,\n    KeyWalk,\n    MouseWalk\n}\n\nexport enum VoiceAction {\n    Create,\n    Join,\n}\n\nexport enum RtcState {\n    Idle = 1, // 未连接语音\n    Connecting = 2, // 连接中\n    Connected = 3, // 已连接\n    Error = 0, // 连接出错\n    Disconnected = 4 // 断开\n}\n\nexport default class PlayerUnit extends MapUnit {\n    public static ROLE_WIDTH: number = 55;\n    public static ROLE_HEIGHT: number = 80;\n    //角色动作4方向\n    public static AVATAR_DIR_NUM: number = 4;\n    //行走速度(像素/秒)\n    public static WARLK_SPEED: number = 400;\n    //交互距离\n    public static INTRACT_DISTANCE: number = 4;\n\n    public playerVo: PlayerVo;\n    private avatar: GameAni;\n    private head: editor.map.PlayerHeadUI;\n    status: PlayerStatus;\n    private walkDir: number;\n    private pathList: Array<Array<number>>;\n    private updateLocationList: Array<object>;\n    private targetCoord: Point;\n    private lastStandTile: Point;\n    private lastLocation: object;\n    public curVoiceUser: PlayerUnit;\n\n    constructor(mapScene: MapScene) {\n        super(mapScene);\n        this.hitArea = new Rectangle(-PlayerUnit.ROLE_WIDTH / 2, -PlayerUnit.ROLE_HEIGHT, PlayerUnit.ROLE_WIDTH, PlayerUnit.ROLE_HEIGHT);\n        this.avatar = new GameAni();\n        this.avatar.on(AniEvent.PLAY_END, this, this.onAniPlayEnd);\n        this.addChild(this.avatar);\n        this.head = new editor.map.PlayerHeadUI();\n        this.addChild(this.head);\n        this.head.x = -this.head.width / 2;\n        this.head.y = -PlayerUnit.ROLE_HEIGHT - this.head.height;\n        this.status = PlayerStatus.Idle;\n        this.pathList = [];\n        this.updateLocationList = [];\n        this.targetCoord = null;\n        this.lastStandTile = null;\n    }\n\n    private onAniPlayEnd(): void {\n        switch (this.avatar.action) {\n            case AniAction.Beckon:\n            case AniAction.Applaud:\n            case AniAction.Ease:\n            case AniAction.Cheer:\n            case AniAction.Celebrate:\n            case AniAction.Embrace:\n                this.doAction(AniAction.Idle, this.avatar.currentDir);\n                break;\n        }\n    }\n\n    private getWalkDistance(time: number, radian: number): Point {\n        var second: number = time / TimeUtils.oneSecondMs;\n        var distanceX: number = Math.cos(radian) * PlayerUnit.WARLK_SPEED * second;\n        var distanceY: number = Math.sin(radian) * PlayerUnit.WARLK_SPEED * second;\n        return new Point(distanceX, distanceY);\n    }\n\n    private getKeyWalkDistance(time: number): Point {\n        var angle: number = 0;\n        switch (this.walkDir) {\n            case RoleDir.Up:\n                angle = 270;\n                break;\n            case RoleDir.Right:\n                angle = 0;\n                break;\n            case RoleDir.Down:\n                angle = 90;\n                break;\n            case RoleDir.Left:\n                angle = 180;\n                break;\n        }\n        var radian: number = MathUtils.angleToRadian(angle);\n        return this.getWalkDistance(time, radian);\n    }\n\n    private getMouseWalkDistance(time: number): Point {\n        var coord: Point = this.targetCoord;\n        var radian: number = MathUtils.radian(new Point(this._virtualX, this._virtualY), coord);\n        return this.getWalkDistance(time, radian);\n    }\n\n    public tick(time: number): void {\n        super.tick(time);\n        if (this.status == PlayerStatus.KeyWalk) {\n            const walkDistance: Point = this.getKeyWalkDistance(time);\n            var nextX: number = this._virtualX + walkDistance.x;\n            var nextY: number = this._virtualY + walkDistance.y;\n            var tile: Point = this.mapScene.getPosTile(nextX, nextY);\n\n            var isBlock: boolean = this.mapScene.isBlock(tile.x, tile.y) || this.mapScene.isPlayerTile(tile.x, tile.y);\n            // TODO: tricky方案 要找到真实原因\n            if (this.walkDir == RoleDir.Up) {\n                isBlock = this.mapScene.isBlock(tile.x, tile.y - 1) || this.mapScene.isPlayerTile(tile.x, tile.y);\n            }\n            if (!isBlock) {\n                this._virtualX = nextX;\n                this._virtualY = nextY;\n            }\n        } else if (this.status == PlayerStatus.MouseWalk) {\n            if (this.targetCoord) {\n                const walkDistance: Point = this.getMouseWalkDistance(time);\n                var leaveDistance: number = MathUtils.distance(this._virtualX, this._virtualY, this.targetCoord.x, this.targetCoord.y);\n                if (MathUtils.getPointLength(walkDistance) >= leaveDistance) {\n                    this._virtualX = this.targetCoord.x;\n                    this._virtualY = this.targetCoord.y;\n                    this.moveToNextTile();\n                } else {\n                    this._virtualX = this._virtualX + walkDistance.x;\n                    this._virtualY = this._virtualY + walkDistance.y;\n                }\n            }\n        } else if (this.status == PlayerStatus.UpdateLocation) {\n            if (this.targetCoord) {\n                Logger.log(\"onMessage targetCoord: \" + this.targetCoord)\n                const walkDistance: Point = this.getMouseWalkDistance(time);\n                var leaveDistance: number = MathUtils.distance(this._virtualX, this._virtualY, this.targetCoord.x, this.targetCoord.y);\n                if (MathUtils.getPointLength(walkDistance) >= leaveDistance) {\n                    this._virtualX = this.targetCoord.x;\n                    this._virtualY = this.targetCoord.y;\n                    this.excuteNextLocation();\n                } else {\n                    this._virtualX = this._virtualX + walkDistance.x;\n                    this._virtualY = this._virtualY + walkDistance.y;\n                }\n            } else {\n                Logger.log(\"onMessage excuteNextLocation\")\n                this.excuteNextLocation();\n            }\n        }\n        //主用户改变位置后广播\n        var standTile: Point = this.getStandTile();\n        if (!this.lastStandTile) {\n            this.lastStandTile = standTile;\n        } else {\n            if (this.lastStandTile.x != standTile.x || this.lastStandTile.y != standTile.y) {\n                if (this.playerVo.game_uid == ModelUser.ins.game_uid && this.status == PlayerStatus.KeyWalk) {\n                    this.broadcastLocation();\n                }\n                GameManager.ins.event(PlayerEvent.PLAYER_MOVE, this);\n                this.lastStandTile = standTile;\n            }\n        }\n        this.zOrder = this._virtualY;\n    }\n\n    private getDirByRadian(radian: number): number {\n        var dir: number = 0;\n        var angle: number = MathUtils.fixAngle(MathUtils.radianToangle(radian));\n        if (angle > 225 && angle <= 315) {\n            dir = RoleDir.Up;\n        } else if (angle > 315 || angle <= 45) {\n            dir = RoleDir.Right;\n        } else if (angle > 45 && angle <= 135) {\n            dir = RoleDir.Down;\n        } else if (angle > 135 && angle <= 225) {\n            dir = RoleDir.Left;\n        }\n        return dir;\n    }\n\n    public init(playerVo: PlayerVo): void {\n        this.playerVo = playerVo;\n        this.head.labName.color = this.playerVo.color;\n        this.head.labName.text = this.playerVo.nickname;\n        this.head.namebg.width = this.head.labName.width + 50;\n        this.head.boxBehavior.visible = false;\n        this.avatar.init(this.playerVo.roleConfig.model, true, PlayerUnit.AVATAR_DIR_NUM);\n        this.setTilePos(this.playerVo.x, this.playerVo.y);\n        this.doAction(AniAction.Idle, this.avatar.currentDir, true, true);\n    }\n\n    public doAction(action: string, dir: RoleDir, loop: boolean = true, force: boolean = false): void {\n        this.avatar.play(action, loop, force, 0, dir);\n    }\n\n    public updateLocation(x: number, y: number, direction: number, time: number): void {\n        var data: object = {};\n        data[\"x\"] = x;\n        data[\"y\"] = y;\n        data[\"direction\"] = direction;\n        data[\"time\"] = time;\n        this.updateLocationList.push(data);\n        this.status = PlayerStatus.UpdateLocation;\n        // this.targetCoord=this.mapScene.getTilePos(x,y);\n        // var dir:number=this.getFaceToDir(this.targetCoord.x,this.targetCoord.y);\n        // this.doAction(AniAction.Walk,dir);\n    }\n\n    public startMove(x: number, y: number, direction: number): void {\n        if (direction == RoleDir.None) {\n            var standTile: Point = this.getStandTile();\n            var path: Array<Array<number\n            >>= this.mapScene.aStar.find(standTile.x, standTile.y, x, y);\n            if (path && path.length > 0) {\n                this.status = PlayerStatus.MouseWalk;\n                this.pathList = path;\n                this.moveToNextTile();\n                this.broadcastFinalLocation(x, y)\n            }\n        } else {\n            this.status = PlayerStatus.KeyWalk;\n            this.walkDir = direction;\n            var oldDir: number = this.avatar.currentDir;\n            this.doAction(AniAction.Walk, direction);\n            if (oldDir != direction) {\n                this.broadcastLocation();\n            }\n        }\n    }\n\n    private moveToNextTile(): void {\n        if (this.pathList.length > 0) {\n            var tile: Array<number> = this.pathList.shift();\n            this.targetCoord = this.mapScene.getTilePos(tile[0], tile[1]);\n            var dir: number = this.getFaceToDir(this.targetCoord.x, this.targetCoord.y);\n            this.doAction(AniAction.Walk, dir);\n        } else {\n            this.stopMove(RoleDir.None);\n        }\n    }\n\n    private excuteNextLocation(): void {\n        if (this.updateLocationList.length > 0) {\n            Logger.log(\"onMessage updateLocationList size: \" + this.updateLocationList.length)\n            var data: object = this.updateLocationList.shift();\n            var time: number = data[\"time\"];\n            var now: number = Laya.Browser.now();\n            var dir: number = data[\"direction\"];\n            Logger.log(\"onMessage dir: \" + dir)\n            if (now - time > 5000) {\n                Logger.log(\"onMessage > 50000\")\n                if (this.updateLocationList.length == 0) {\n                    var lastCoord: Point = this.mapScene.getTilePos(data[\"x\"], data[\"y\"]);\n                    this._virtualX = lastCoord.x;\n                    this._virtualY = lastCoord.y;\n                    this.stopMove(dir);\n                } else {\n                    this.lastLocation = data;\n                    this.excuteNextLocation();\n                }\n            } else {\n                Logger.log(\"onMessage: \" + this.lastLocation)\n                if (this.lastLocation) {\n                    var lastCoord: Point = this.mapScene.getTilePos(this.lastLocation[\"x\"], this.lastLocation[\"y\"]);\n                    this._virtualX = lastCoord.x;\n                    this._virtualY = lastCoord.y;\n                    this.lastLocation = null;\n                }\n\n                this.targetCoord = this.mapScene.getTilePos(data[\"x\"], data[\"y\"]);\n                this.doAction(AniAction.Walk, dir);\n            }\n        } else {\n            this.stopMove(RoleDir.None);\n        }\n    }\n\n    public broadcastFinalLocation(targetX: number, targetY: number): void {\n        var dir: number = this.avatar.currentDir;\n        GameHandler.ins.send_updateLocation(targetX, targetY, dir);\n    }\n\n    public broadcastLocation(): void {\n        var standTile: Point = this.getStandTile();\n        var dir: number = this.avatar.currentDir;\n        GameHandler.ins.send_updateLocation(standTile.x, standTile.y, dir);\n    }\n\n    public stopMove(direction: number): void {\n        //键盘行走的时候避免卡顿\n        if (this.status == PlayerStatus.KeyWalk && this.walkDir != direction) {\n            return;\n        }\n        this.pathList.length = 0;\n        this.targetCoord = null;\n        this.walkDir = RoleDir.None;\n        this.status = PlayerStatus.Idle;\n        this.doAction(AniAction.Idle, this.avatar.currentDir);\n    }\n\n    private hideFace(): void {\n        this.head.boxBehavior.visible = false;\n    }\n\n    private showFace(faceId: number): void {\n        var faceConfig: FaceConfig = FaceTable.ins.getConfig(faceId);\n        if (faceConfig) {\n            Laya.timer.clear(this, this.hideFace);\n            this.head.boxBehavior.visible = true;\n            this.head.imgSp.visible = true;\n            this.head.labelSp.visible = false;\n            this.head.imgFace.skin = faceConfig.img;\n            Laya.timer.once(1 * TimeUtils.oneSecondMs, this, this.hideFace);\n        }\n    }\n\n    public setHitArea(clear: boolean): void {\n        if (clear) {\n            this.hitArea = null;\n        } else {\n            this.hitArea = new Rectangle(-PlayerUnit.ROLE_WIDTH / 2, -PlayerUnit.ROLE_HEIGHT, PlayerUnit.ROLE_WIDTH, PlayerUnit.ROLE_HEIGHT);\n        }\n    }\n\n    public showHeadText(text: string): void {\n        this.head.boxBehavior.visible = true;\n        this.head.imgSp.visible = false;\n        this.head.labelSp.visible = true;\n        this.head.labelText.text = text;\n        this.head.labelBg.width = this.head.labelText.textField.textWidth + 20;\n        this.head.labelBg.height = this.head.labelText.textField.height + 20;\n        this.head.bgsanjiao.y = this.head.labelBg.height * 0.48;\n        Laya.timer.clear(this, this.hideFace);\n        Laya.timer.once(2 * TimeUtils.oneSecondMs, this, this.hideFace);\n    }\n\n    private perform(performId: number): void {\n        var performConfig: PerformConfig = PerformTable.ins.getConfig(performId);\n        if (performConfig) {\n            this.doAction(performConfig.params, this.avatar.currentDir, false);\n        }\n    }\n\n    public doBehavior(behavior_id: number, params: string): void {\n        if (behavior_id == BehaviorType.SendFace) {\n            this.showFace(parseInt(params));\n        } else if (behavior_id == BehaviorType.ClearFace) {\n            this.hideFace();\n        } else if (behavior_id == BehaviorType.Perform) {\n            this.perform(parseInt(params));\n        }\n    }\n\n    public doInteract(interact_id: number, target_uid: string, params: string): void {\n        var config: InteractConfig = InteractTable.ins.getConfig(interact_id);\n        if (config) {\n            if (config.type == InteractType.SendMsg) {\n                this.showHeadText(params);\n            } else if (config.type == InteractType.DoAction) {\n                var targetPlayer: PlayerUnit = this.mapScene.getPlayer(target_uid);\n                if (targetPlayer) {\n                    //面向目标\n                    var dir: number = this.getFaceToDir(targetPlayer.virtualX, targetPlayer.virtualY);\n                    if (config.params == AniAction.Throw) {\n                        this.doAction(AniAction.Embrace, dir, false);\n                    } else {\n                        this.doAction(config.params, dir, false);\n                    }\n                    if (targetPlayer) {\n                        targetPlayer.beInteract(this, interact_id, params);\n                    }\n                    if (config.params == AniAction.Embrace) {\n                        var midPos: Point = new Point((this.virtualX + targetPlayer.virtualX) * 0.5, (this.virtualY + targetPlayer.virtualY) * 0.5);\n                        this.mapScene.playEffect(AnimationKey.aixin, midPos.x, midPos.y, 0);\n                    }\n                    if (config.params == AniAction.Throw) {\n                        this.mapScene.playEffect(AnimationKey.jidan, targetPlayer.virtualX, targetPlayer.virtualY - PlayerUnit.ROLE_HEIGHT * 0.5, 3000);\n                    }\n                }\n            }\n        }\n    }\n\n    public beInteract(orignal: PlayerUnit, interact_id: number, params: string): void {\n        if (this.status != PlayerStatus.Idle) return;\n        var config: InteractConfig = InteractTable.ins.getConfig(interact_id);\n        if (config) {\n            if (config.params == AniAction.Throw) {\n                return;\n            }\n            //面向源\n            var dir: number = this.getFaceToDir(orignal.virtualX, orignal.virtualY);\n            this.doAction(config.params, dir, false);\n        }\n    }\n\n    public onClick(): void {\n        if (EntityManager.ins.status != EntityBuildStatus.None) {\n            return;\n        }\n        if (this.status != PlayerStatus.Idle) {\n            FloatMessage.showMessage(\"空闲状态下才能操作\");\n            return;\n        }\n        //点的是自己\n        if (this.playerVo.game_uid == ModelUser.ins.game_uid) {\n            var handleRefer: Point = new Point(0.5, 1);\n            var handleTipUI: Point = new Point(0.5, 0);\n            var locationVo: LocationVo = new LocationVo(this.avatar, handleRefer, handleTipUI);\n            TipUIManager.ins.showTipUI(TipUIKey.PerformTip, null, locationVo);\n        } else {\n            var myStandTile: Point = this.mapScene.mainPlayer.getStandTile();\n            var targetStandTile: Point = this.getStandTile();\n            if (myStandTile.distance(targetStandTile.x, targetStandTile.y) > PlayerUnit.INTRACT_DISTANCE) {\n                FloatMessage.showMessage(\"距离过远无法互动\");\n                return;\n            }\n            var handleRefer: Point = new Point(0.5, 1);\n            var handleTipUI: Point = new Point(0.5, 0);\n            var locationVo: LocationVo = new LocationVo(this.avatar, handleRefer, handleTipUI);\n            TipUIManager.ins.showTipUI(TipUIKey.InteractTip, this, locationVo);\n            this.mapScene.cacheInteractUid = this.playerVo.game_uid;\n        }\n    }\n\n    public checkIntractDis(): boolean {\n        var myStandTile: Point = this.mapScene.mainPlayer.getStandTile();\n        var targetStandTile: Point = this.getStandTile();\n        if (myStandTile.distance(targetStandTile.x, targetStandTile.y) > PlayerUnit.INTRACT_DISTANCE) {\n            return false;\n        } else {\n            return true;\n        }\n    }\n\n    private static generateVoiceRoomId(player: PlayerUnit, nearestPlayer: PlayerUnit): string {\n        if (player.playerVo.roomId != \"\") {\n            return player.playerVo.roomId.trim() + \"-\" + \"subAudioRoom\"\n        } else {\n            return player.playerVo.game_uid + \"-\" + nearestPlayer.playerVo.game_uid + \"-\" + \"subAudioRoom\"\n        }\n    }\n\n    private isNearestPlayer(targetPlayer: PlayerUnit): boolean {\n        const myTile = this.getStandTile()\n        const targetTile = targetPlayer.getStandTile()\n        if (myTile.distance(targetTile.x, targetTile.y) < PlayerUnit.INTRACT_DISTANCE) {\n            return true\n        }\n        return false\n    }\n\n    private joinSubRoom(nearestPlayer: PlayerUnit) {\n        this.playerVo.rtcState = RtcState.Connecting\n        // 拼装audio_room_id\n        this.playerVo.audioRoomId = PlayerUnit.generateVoiceRoomId(this, nearestPlayer)\n\n        // 调用web创建语音房接口,等待回调\n        GameHandler.ins.send_joinSubRoom(this.playerVo.audioRoomId, this.playerVo.game_uid, nearestPlayer.playerVo.game_uid)\n        GameHandler.ins.send_userInfoChanged(this.playerVo) // 广播自己的rtc状态\n    }\n\n    private createSubRoom(nearestPlayer: PlayerUnit, voiceAction: VoiceAction) {\n        this.playerVo.rtcState = RtcState.Connecting\n        // 拼装audio_room_id\n        this.playerVo.audioRoomId = PlayerUnit.generateVoiceRoomId(this, nearestPlayer)\n\n        // 调用web创建语音房接口,等待回调\n        if (voiceAction == VoiceAction.Create) {\n            GameHandler.ins.send_joinSubRoom(this.playerVo.audioRoomId, this.playerVo.game_uid, nearestPlayer.playerVo.game_uid)\n        } else if (voiceAction == VoiceAction.Join) {\n            GameHandler.ins.send_joinSubRoom(this.playerVo.audioRoomId, this.playerVo.game_uid, null)\n        }\n\n        GameHandler.ins.send_userInfoChanged(this.playerVo) // 广播自己的rtc状态\n    }\n\n    public leaveSubRoom() {\n        return\n        // if (!GlobalVars.isWebGame) {\n        //     return;\n        // }\n        FloatMessage.showMessage(\"断开语音连接\")\n        const audioRoomID = this.playerVo.audioRoomId // 保存audioRoomId用于断开语音房\n\n        this.playerVo.rtcState = RtcState.Idle\n        this.playerVo.audioRoomId = \"\"\n        this.playerVo.roomId = \"\"\n        GameHandler.ins.send_userInfoChanged(this.playerVo)\n\n        GameHandler.ins.send_leaveSubRoom(audioRoomID, this.playerVo.game_uid)\n    }\n\n    public disconnectVoice(originPlayer: PlayerUnit) {\n        return;\n        // if (!GlobalVars.isWebGame) {\n        //     return;\n        // }\n        if (this.playerVo.roomId != \"\") {\n            // 房间情况下，player还没离开房间\n            return;\n        }\n        if (this.playerVo.rtcState != RtcState.Connected) {\n            // 房间语音情况下，playerMove也会调到，这里判断一下状态，直接退出\n            return;\n        }\n\n        if (this.playerVo.audioRoomId != \"\" && this.playerVo.roomId != \"\" && this.playerVo.audioRoomId.includes(this.playerVo.roomId)) {\n            FloatMessage.showMessage(\"在同一房间，不断开语音\")\n            return\n        }\n        if (originPlayer == null) {\n            this.leaveSubRoom()\n        } else {\n            FloatMessage.showMessage(\"还在附近，不断开\")\n        }\n    }\n\n    public createOrJoinVoice(nearestPlayer: PlayerUnit) {\n        return;\n        // if (!GlobalVars.isWebGame) {\n        //     return;\n        // }\n        Logger.log(\"zhgli createOrJoinVoice: \" + nearestPlayer + \" \" + this.playerVo.roomId)\n        if (nearestPlayer == null && this.playerVo.roomId != \"\") {\n            // player进入了房间区域\n            if (this.playerVo.rtcState != RtcState.Idle) {\n                return;\n            }\n            // 检查房间中人数\n            let playerCountInSpecialRoom = 0;\n            let nearestPlayerInRoom: PlayerUnit\n            for (const key in this.mapScene.getPlayerList()) {\n                const player: PlayerUnit = this.mapScene.getPlayerList()[key]\n                if (player != null && player.playerVo.roomId == this.playerVo.roomId) {\n                    playerCountInSpecialRoom++\n                    if (player.playerVo.game_uid != this.playerVo.game_uid) {\n                        nearestPlayerInRoom = player\n                    }\n                }\n            }\n            if (playerCountInSpecialRoom == 1) {\n                // 只有自己，啥也不干\n                // FloatMessage.showMessage(\"只有自己\")\n            } else if (playerCountInSpecialRoom == 2) {\n                // 需要创建语音连接了\n                this.createSubRoom(nearestPlayerInRoom, VoiceAction.Create)\n                // FloatMessage.showMessage(\"创建语音房\")\n            } else if (playerCountInSpecialRoom > 2) {\n                // 直接加入语音\n                this.joinSubRoom(nearestPlayer)\n                // FloatMessage.showMessage(\"加入语音房\")\n            }\n            return;\n        }\n\n        if (this.playerVo.roomId != \"\") {\n            // 房间内人物碰撞，不处理\n            return;\n        }\n\n        Logger.log(\"zhgli nearestPlayer: \" + nearestPlayer.playerVo.nickname + \" \" + nearestPlayer.playerVo.rtcState + \" \" + this.playerVo.rtcState)\n\n        if (!this.isNearestPlayer(nearestPlayer)) {\n            // FloatMessage.showMessage(\"已经走远了\")\n            return;\n        }\n        // 有创建或加入的意图\n        switch (nearestPlayer.playerVo.rtcState) {\n            case RtcState.Idle:\n                // 创建语音房\n                // FloatMessage.showMessage(\"与\" + nearestPlayer.playerVo.nickname + \"创建语音房\")\n                this.createSubRoom(nearestPlayer, VoiceAction.Create)\n                break\n            case RtcState.Connecting:\n                // 对方正在建立语音连接\n                // FloatMessage.showMessage(nearestPlayer.playerVo.nickname + \"正在连接语音\")\n                break\n            case RtcState.Connected:\n                // 对方已在语音房，加入\n                // FloatMessage.showMessage(\"加入\" + nearestPlayer.playerVo.nickname + \"的语音房: \" + nearestPlayer.playerVo.audioRoomId)\n                this.createSubRoom(nearestPlayer, VoiceAction.Join)\n                break\n        }\n    }\n\n    public showIntractTip(): void {\n        if (GlobalVars.isWebGame) {\n            FilterUtils.setColorFilter(this.avatar, 2, 1, 1);\n            this.head.tipBox.visible = true;\n            this.clearTimer(this, this.hideIntractTip);\n            this.timerOnce(2000, this, this.hideIntractTip);\n        }\n    }\n\n    public hideIntractTip(): void {\n        FilterUtils.setColorFilter(this.avatar, 1, 1, 1);\n        this.head.tipBox.visible = false;\n    }\n\n    //获取面向坐标的朝向枚举\n    public getFaceToDir(x: number, y: number): number {\n        var stand: Point = new Point(this._virtualX, this._virtualY);\n        var target: Point = new Point(x, y);\n        var radian: number = MathUtils.radian(stand, target);\n        return this.getDirByRadian(radian);\n    }\n\n    public destroy(destroyChild: boolean = true) {\n        this.avatar.off(AniEvent.PLAY_END, this, this.onAniPlayEnd);\n        Laya.timer.clear(this, this.hideFace);\n        this.clearTimer(this, this.hideIntractTip);\n        this.head.destroy();\n        this.avatar.destroy();\n        this.playerVo = null;\n        super.destroy(destroyChild);\n    }\n}","import RoleTable, { RoleConfig } from \"../../configs/role/RoleTable\";\nimport {RtcState} from \"../../map/PlayerUnit\";\n\nexport default class PlayerVo{\n    public nickname:string;\n    public role_id:number;\n    public game_uid:string;\n    public x:number;\n    public y:number;\n    public direction:number;\n    public color:string;\n    public roomId: string; // 地图上mask名称\n    public rtcState: RtcState // 语音连接状态\n    public audioRoomId: string; // rtc id\n\n    constructor(){\n        this.nickname=\"\";\n        this.role_id=0;\n        this.game_uid=\"\";\n        this.x=0;\n        this.y=0;\n        this.direction=0;\n        this.color = \"#FFFFFF\";\n        this.roomId = \"\"\n        this.rtcState = RtcState.Idle\n        this.audioRoomId = \"\"\n    }\n\n\tpublic get roleConfig():RoleConfig{\n\t\treturn RoleTable.ins.getConfig(this.role_id);\n\t}\n}","import SheetConfig from '../core/SheetConfig';\nimport ConfigManager from '../../managers/ConfigManager';\nimport ConfigKey from '../../constant/ConfigKey';\nimport StructConfig from '../core/StructConfig';\nimport LangUtils from '../../utils/LangUtils';\nimport PathManager from '../../managers/PathManager';\nexport class EntityConfig extends StructConfig{\n    //Id\n    public id:number;\n    //名称\n    private name:string;\n    //资源\n    public res:string;\n    //横向占用格子数\n    public hGridNum:number;\n    //纵向占用格子数\n    public vGridNum:number;\n    //是否是地板\n    public isFloor:boolean;\n}\nexport default class EntityTable extends SheetConfig {\n\tprivate static _ins: EntityTable;\n\tpublic static get ins(): EntityTable {\n\t\treturn EntityTable._ins = EntityTable._ins || new EntityTable();\n\t}\n\tconstructor(){\n        super();\n        ConfigManager.analyzeConfig(ConfigKey.entity,this);\n    }\n\tprotected get recordCls(): any {\n\t\treturn EntityConfig;\n\t}\n}","import EntityTable, { EntityConfig } from \"../../configs/entity/EntityTable\";\n\nexport default class EntityVo{\n    public guid:number;\n    public builder_uid:string;\n    public entity_id:number;\n    public x:number;\n    public y:number;\n    constructor(){\n        this.guid=0;\n        this.builder_uid=\"\";\n        this.entity_id=0;\n        this.x=0;\n        this.y=0;\n    }\n    public get entityConfig():EntityConfig{\n        return EntityTable.ins.getConfig(this.entity_id);\n    }\n}","import PlayerVo from \"../../model/map/PlayerVo\";\nimport EntityVo from \"../../model/map/EntityVo\";\nimport {RtcState} from \"../../map/PlayerUnit\";\n\nexport default class ProtocolUtils {\n    public static createPlayerVo(data: any): PlayerVo {\n        var playerVo: PlayerVo = new PlayerVo();\n        playerVo.nickname = data[\"nickname\"];\n        playerVo.role_id = data[\"role_id\"];\n        playerVo.game_uid = data[\"game_uid\"];\n        playerVo.x = data[\"x\"];\n        playerVo.y = data[\"y\"];\n        playerVo.direction = data[\"direction\"];\n        playerVo.rtcState = data[\"rtcState\"]\n        if (playerVo.rtcState == null) {\n            playerVo.rtcState = RtcState.Idle\n        }\n        playerVo.audioRoomId = data[\"audioRoomId\"]\n        if (playerVo.audioRoomId == null) {\n            playerVo.audioRoomId = \"\"\n        }\n        playerVo.roomId = data[\"roomId\"]\n        if (playerVo.roomId == null) {\n            playerVo.roomId = \"\"\n        }\n        if (data[\"color\"] != null) {\n            playerVo.color = data[\"color\"];\n        }\n        return playerVo;\n    }\n\n    public static createEntityVo(data: any): EntityVo {\n        var entityVo: EntityVo = new EntityVo();\n        entityVo.guid = data[\"guid\"];\n        entityVo.builder_uid = data[\"builder_uid\"];\n        entityVo.entity_id = data[\"entity_id\"];\n        entityVo.x = data[\"x\"];\n        entityVo.y = data[\"y\"];\n        return entityVo;\n    }\n}","export default class InitEvent{\n    public static INIT_ROOM:string=\"INIT_ROOM\";\n}","import GameProtocol from \"../core/GameProtocol\";\nimport ModelUser from \"../../model/user/ModelUser\";\nimport ProtocolUtils from '../utils/ProtocolUtils';\nimport ModelMap from '../../model/map/ModelMap';\nimport EntityVo from '../../model/map/EntityVo';\nimport PlayerVo from \"../../model/map/PlayerVo\";\nimport GameManager from '../../managers/GameManager';\nimport MapEvent from \"../../events/MapEvent\";\nimport KeyEvent from \"../../events/KeyEvent\";\nimport InitEvent from '../../events/InitEvent';\nimport Logger from \"../../utils/Logger\";\nimport {RtcState} from \"../../map/PlayerUnit\";\nimport Handler = Laya.Handler;\nimport Browser = Laya.Browser;\nimport GlobalVars from \"../../GlobalVars\";\n\nexport enum ProtocolAction {\n    initRoom = \"initRoom\",\n    addUser = \"addUser\",\n    removeUser = \"removeUser\",\n    keyAction = \"keyAction\",\n    keyDown = \"keyDown\",\n    keyUp = \"keyUp\",\n    updateLocation = \"updateLocation\",\n    interact = \"interact\",\n    behavior = \"behavior\",\n    buildEntity = \"buildEntity\",\n    addEntity = \"addEntity\",\n    removeEntity = \"removeEntity\",\n    joinSubRoom = \"joinSubRoom\",\n    leaveSubRoom = \"leaveSubRoom\",\n    userInfoChanged = \"userInfoChanged\",\n}\n\nexport default class GameHandler {\n    public init(): void {\n        GameProtocol.ins.registerHandler(ProtocolAction.initRoom, new Handler(this, this.respond_initRoom));\n        GameProtocol.ins.registerHandler(ProtocolAction.addUser, new Handler(this, this.respond_addUser));\n        GameProtocol.ins.registerHandler(ProtocolAction.removeUser, new Handler(this, this.respond_removeUser));\n        GameProtocol.ins.registerHandler(ProtocolAction.keyAction, new Handler(this, this.respond_keyAction));\n        GameProtocol.ins.registerHandler(ProtocolAction.keyDown, new Handler(this, this.respond_keyDown));\n        GameProtocol.ins.registerHandler(ProtocolAction.keyUp, new Handler(this, this.respond_keyUp));\n        GameProtocol.ins.registerHandler(ProtocolAction.updateLocation, new Handler(this, this.respond_updateLocation));\n        GameProtocol.ins.registerHandler(ProtocolAction.behavior, new Handler(this, this.respond_behavior));\n        GameProtocol.ins.registerHandler(ProtocolAction.interact, new Handler(this, this.respond_interact));\n        GameProtocol.ins.registerHandler(ProtocolAction.addEntity, new Handler(this, this.respond_addEntity));\n        GameProtocol.ins.registerHandler(ProtocolAction.removeEntity, new Handler(this, this.respond_removeEntity));\n        GameProtocol.ins.registerHandler(ProtocolAction.joinSubRoom, new Handler(this, this.respond_joinSubRoom))\n        GameProtocol.ins.registerHandler(ProtocolAction.leaveSubRoom, new Handler(this, this.respond_leaveSubRoom))\n        GameProtocol.ins.registerHandler(ProtocolAction.userInfoChanged, new Handler(this, this.respond_userInfoChanged))\n    }\n\n    public send_userInfoChanged(data: object) {\n        GameProtocol.ins.sendMessage(ProtocolAction.userInfoChanged, data)\n        ModelMap.ins.event(MapEvent.USER_INFO_CHANGED, data)\n    }\n\n    public respond_userInfoChanged(data: object) {\n        const playerVo: PlayerVo = ProtocolUtils.createPlayerVo(data);\n        Logger.log(\"zhgli respond_userInfoChanged: \" + playerVo.nickname + \" audioRoomId: \" + playerVo.audioRoomId + \" rtcState: \" + playerVo.rtcState + \" roomId: \" + playerVo.roomId)\n        ModelMap.ins.event(MapEvent.USER_INFO_CHANGED, playerVo)\n    }\n\n    public send_joinSubRoom(sub_roomid: string, master_uid: string, slave_id: string) {\n        var data: object = {};\n        data[\"sub_roomid\"] = sub_roomid;\n        data[\"game_uid\"] = master_uid;\n        data[\"target_id\"] = slave_id\n        GameProtocol.ins.sendMessage(ProtocolAction.joinSubRoom, data);\n    }\n\n    public respond_joinSubRoom(data: object) {\n        if (data == null) {\n            return;\n        }\n        Logger.log(\"zhgli respond_joinSubRoom: \" + data[\"rtcState\"])\n        const game_uid = data[\"game_uid\"] // 语音发起方\n        const target_id = data[\"target_id\"] // 语音被动方\n        const sub_roomid = data[\"sub_roomid\"]\n        const rtcState: RtcState = data[\"rtcState\"]\n        Logger.log(\"zhgli target_id: \" + target_id)\n        Logger.log(\"zhgli sub_roomid: \" + sub_roomid)\n        Logger.log(\"zhgli rtcState: \" + rtcState)\n        if (target_id == null) {\n            return\n        }\n\n        if (target_id == ModelUser.ins.game_uid) {\n            // 自己是被动方,要去加入语音房\n            const player = ModelMap.ins.getPlayer(ModelUser.ins.game_uid)\n            player.rtcState = rtcState\n            player.audioRoomId = sub_roomid\n            this.send_userInfoChanged(player)\n            // TODO: 在未连接的情况下，web有时候会返回rtcState = 3 是有问题的，导致语音没有连接\n            if (player.rtcState == RtcState.Idle) {\n                Logger.log(\"zhgli \" + ModelUser.ins.nickname + \" joinsubRoom\")\n                // 连接过程中会多次回调，避免多次调用加入语音房\n                this.send_joinSubRoom(sub_roomid, ModelUser.ins.game_uid, \"\")\n            }\n        } else if (game_uid == ModelUser.ins.game_uid) {\n            Logger.log(\"zhgli \" + ModelUser.ins.nickname + \" create room success, broadcast rtcState: \" + rtcState)\n            // 自己是创建语音房的一方，更新自己的状态\n            const player = ModelMap.ins.getPlayer(ModelUser.ins.game_uid)\n            player.rtcState = rtcState\n            player.audioRoomId = sub_roomid\n            // 查看连接状态，更新自己的playervo,并广播出去\n            this.send_userInfoChanged(player)\n        }\n    }\n\n    public send_leaveSubRoom(sub_roomid: string, game_uid: string) {\n        if (sub_roomid == null || game_uid == null) {\n            Logger.log(\"zhgli send_leaveSubRoom: \" + sub_roomid + \" \" + game_uid)\n            return\n        }\n        var data: object = {};\n        data[\"game_uid\"] = game_uid;\n        data[\"sub_roomid\"] = sub_roomid;\n        GameProtocol.ins.sendMessage(ProtocolAction.leaveSubRoom, data);\n    }\n\n    public respond_leaveSubRoom(data: object) {\n        Logger.log(\"respond_leaveSubRoom\")\n        ModelMap.ins.event(MapEvent.USER_LEAVE_ROOM, data)\n    }\n\n    public send_behavior(behavior_id: number, params: string): void {\n        var data: object = {};\n        data[\"game_uid\"] = ModelUser.ins.game_uid;\n        data[\"behavior_id\"] = behavior_id;\n        data[\"params\"] = params;\n        GameProtocol.ins.sendMessage(ProtocolAction.behavior, data);\n    }\n\n    private respond_behavior(data: object): void {\n        ModelMap.ins.event(MapEvent.BEHAVIOR, data);\n    }\n\n    public send_interact(interact_id: number, target_uid: string, params: string = \"\"): void {\n        var data: object = {};\n        data[\"game_uid\"] = ModelUser.ins.game_uid;\n        data[\"interact_id\"] = interact_id;\n        data[\"target_uid\"] = target_uid;\n        data[\"params\"] = params;\n        GameProtocol.ins.sendMessage(ProtocolAction.interact, data);\n    }\n\n    private respond_interact(data: object): void {\n        ModelMap.ins.event(MapEvent.INTERACT, data);\n    }\n\n    public send_addUser(data: object): void {\n        GameProtocol.ins.sendMessage(ProtocolAction.addUser, data);\n    }\n\n    private respond_addUser(data: object): void {\n        var playerVo: PlayerVo = ProtocolUtils.createPlayerVo(data);\n        ModelMap.ins.addPlayer(playerVo);\n        ModelMap.ins.event(MapEvent.ADD_PLAYER, playerVo);\n    }\n\n    public send_updateLocation(x: number, y: number, direction: number): void {\n        var data: object = {};\n        data[\"game_uid\"] = ModelUser.ins.game_uid;\n        data[\"nickname\"] = ModelUser.ins.nickname;\n        data[\"role_id\"] = ModelUser.ins.role_id;\n        data[\"x\"] = x;\n        data[\"y\"] = y;\n        data[\"direction\"] = direction;\n        GameProtocol.ins.sendMessage(ProtocolAction.updateLocation, data);\n    }\n\n    public respond_updateLocation(data: object): void {\n        //广播给自己的就不要处理了\n        var game_uid: string = data[\"game_uid\"];\n        var playerVo: PlayerVo = ModelMap.ins.getPlayer(game_uid);\n        if (playerVo) {\n            playerVo.x = data[\"x\"];\n            playerVo.y = data[\"y\"];\n            playerVo.direction = data[\"playerVo\"];\n        }\n        if (game_uid == ModelUser.ins.game_uid) {\n            return;\n        }\n        if (data[\"time\"] == null) {\n            data[\"time\"] = Browser.now();\n        }\n        ModelMap.ins.event(MapEvent.UPDATE_LOCATION, data);\n    }\n\n    public send_buildEntity(entity_id: number, x: number, y: number): void {\n        var data: object = {};\n        data[\"game_uid\"] = ModelUser.ins.game_uid;\n        data[\"entity_id\"] = entity_id;\n        data[\"x\"] = x;\n        data[\"y\"] = y;\n        GameProtocol.ins.sendMessage(ProtocolAction.buildEntity, data);\n    }\n\n    public respond_addEntity(data: object): void {\n        var entityVo: EntityVo = ProtocolUtils.createEntityVo(data);\n        ModelMap.ins.addEntity(entityVo);\n        ModelMap.ins.event(MapEvent.ADD_ENTITY, entityVo);\n    }\n\n    public send_removeEntity(guid: number):